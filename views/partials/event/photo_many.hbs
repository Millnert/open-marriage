{{#each this}}
<div class="event-top-photos-line">
<div class="event-photo-multi">
<ul class="ul-float-elems">
{{#each this}}
<li class="li-floated-elem{{#if rotate}} rotate{{rotate}}{{/if}}">
  <div class="event-photo-multi-img">
    <a href="{{filename}}" target=_blank"><img src="{{filename}}" alt="{{alt}}" title="{{alt}}" height="{{height}}"/></a>
    {{#if credit}}
    <a class="event-photo-credit" href="{{credit.url}}">{{credit.text}}</a>
    {{/if}}
  </div>
</li>
{{/each}}
</ul>
</div>
</div>
{{/each}}
